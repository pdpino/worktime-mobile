{
  "name": "worktime",
  "version": "1.17.1-dev",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "android:clean": "cd android && ./gradlew clean",
    "android": "react-native run-android",
    "log": "react-native log-android",
    "lint": "eslint src",
    "test": "jest",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand",
    "release:deprecated": "react-native run-android --variant=release",
    "release:build": "cd android && ./gradlew assembleRelease",
    "release:send": "./scripts/tg.sh",
    "release:git": "./scripts/git.sh",
    "release": "npm run release:build && npm run release:send && npm run release:git"
  },
  "dependencies": {
    "@notifee/react-native": "^5.1.0",
    "@react-native-community/async-storage": "^1.12.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/netinfo": "^5.9.6",
    "@react-navigation/bottom-tabs": "^6.2.0",
    "@react-navigation/native": "^6.0.8",
    "@react-navigation/native-stack": "^6.5.2",
    "@redux-offline/redux-offline": "^2.6.0-native.0",
    "date-fns": "^2.6.0",
    "date-fns-tz": "^1.0.10",
    "i18n-js": "^3.5.0",
    "lodash": "^4.17.19",
    "memoize-one": "^5.0.4",
    "react": "17.0.1",
    "react-native": "0.64.2",
    "react-native-action-button": "git://github.com/pdpino/react-native-action-button.git#fix/native-driver-warning",
    "react-native-calendars": "^1.20.0",
    "react-native-collapsible": "^1.5.1",
    "react-native-device-info": "^5.6.5",
    "react-native-elements": "^2.0.0",
    "react-native-file-selector": "^1.0.2",
    "react-native-fs": "^2.19.0",
    "react-native-gesture-handler": "^2.3.2",
    "react-native-localize": "^1.3.1",
    "react-native-material-menu": "^1.1.3",
    "react-native-modal": "^11.5.6",
    "react-native-reanimated": "^2.5.0",
    "react-native-safe-area-context": "^4.2.4",
    "react-native-screens": "^3.13.1",
    "react-native-share": "^3.7.0",
    "react-native-svg": "^12.3.0",
    "react-native-vector-icons": "^7.0.0",
    "react-native-week-view": "github:pdpino/react-native-week-view#unstable/0.13.0-dev",
    "react-redux": "^7.2.8",
    "redux": "^4.1.2",
    "redux-orm": "^0.12.1",
    "redux-thunk": "^2.4.1",
    "reselect": "^4.1.5",
    "unorm": "^1.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "eslint": "7.14.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.3.0",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4",
    "factory-girl": "^5.0.4",
    "jest": "^26.6.3",
    "jest-chain": "^1.1.5",
    "jest-extended": "^0.11.2",
    "metro-react-native-babel-preset": "^0.64.0",
    "react-test-renderer": "17.0.1",
    "redux-mock-store": "^1.5.3",
    "timezone-mock": "^1.1.1"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "./src/__tests__/setup.js",
      "./node_modules/react-native-gesture-handler/jestSetup.js"
    ],
    "setupFilesAfterEnv": [
      "jest-extended",
      "jest-chain"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|@react-native|@react-navigation)"
    ],
    "testRegex": "(/__tests__/.*|\\.|/)(test|spec)\\.[jt]sx?$"
  }
}
